{
	"languageserver": { 
		"crystal": { 
			"command": "crystalline",
			"args": [
				"--stdio"
			],
			"filetypes": [ 
				"crystal"
			]
		}
	},
	"rpc.idleText": "Probably fucking off to hentai.",
	"rpc.buttonEnabled": true,
	"rpc.idleTimeout": 120,
	"rpc.largeImage": "Editing a {Lang} file like a idiot",
	"rpc.lowerDetailsEditing": "Editing {file_name} at L{current_line}, fuck!",
	"rpc.detailsEditing": "Sucking with {total_lines} lines",
	"rpc.lowerDetailsViewing": "Viewing {file_name}, fuck off",
	"rpc.problemsText": "{count} skill issue",
	"rpc.detailsViewing": "In {workspace_folder} {problems}, pls ping",
	"rpc.lowerDetailsNotFound": "owo |",
	"rpc.suppressNotifications": false,
  "refactor.openCommand": "vsplit",
  "suggest.autoTrigger": "always",
  "codeLens.enable": true,
  "codeLens.separator": "‣",
  "rpc.buttonInactiveUrl": "https://github.com/taskylizard",
  "rpc.buttonInactiveLabel": "~",
	"python.formatting.provider": "black",
	"python.linting.flake8Enabled": true,
	"pyright.enable": true,
  "python.analysis.useLibraryCodeForTypes": true,
  "coc.preferences.promptInput": false,
  "suggest.timeout": 2000,
  "suggest.completionItemKindLabels": {
    "function": "\uf794",
    "method": "\uf6a6",
    "variable": "\uf71b",
    "constant": "\uf8ff",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\ufa52",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf668",
    "color": "\ue22b",
    "property": "\ufab6",
    "field": "\uf93d",
    "unit": "\uf475",
    "file": "\uf471",
    "value": "\uf8a3",
    "event": "\ufacd",
    "folder": "\uf115",
    "keyword": "\uf893",
    "snippet": "\uf64d",
    "operator": "\uf915",
    "reference": "\uf87a",
    "typeParameter": "\uf278",
    "default": "\uf29c"
  },
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "ஐ",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualTextPrefix": " ❯❯❯ ",
  "diagnostic.virtualText": true,
  "diagnostic.enableMessage": "never",
  "list.source.grep.command": "rg",
  "html.autoClosingTags": true
}
